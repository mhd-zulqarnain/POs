    <declare-styleable name="CalcEraseButton">
        <attr name="calcEraseBtnHoldDelay" format="integer"/>
        <attr name="calcEraseBtnHoldSpeed" format="integer"/>
        <attr name="calcEraseAllOnHold" format="boolean"/>
    </declare-styleable>

</resources>